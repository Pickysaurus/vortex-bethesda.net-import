    "@types/react": "^19.2.8",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/yazl": "^3.3.0",
    "cpy-cli": "^7.0.0",
    "electron": "^40.0.0",
    "fs": "^0.0.1-security",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vortex-api": "Nexus-Mods/vortex-api",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-i18next": "^16.5.3",
