{
  "name": "import-from-bethesda.net",
  "version": "2.0.5",
  "description": "Import mods managed by Bethesda.net into Vortex",
  "main": "./out/index.js",
  "repository": "",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack --config webpack.config.js --progress --profile --color",
    "bundle7z": "7z a -t7z sample-extension.7z .\\dist\\index.js .\\dist\\info.json",
    "build": "npm run webpack && npm run copyassets && extractInfo",
    "copyassets": "cpy \"src/stylesheets/*\" dist --flat && cpy \"src/images/*\" dist --flat"
  },
