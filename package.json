    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "bluebird": "^3.5.5",
    "copyfiles": "^2.1.1",
    "electron": "4.2.4",
    "i18next": "^17.0.4",
    "modmeta-db": "Nexus-Mods/modmeta-db",
    "react": "^16.8.6",
    "react-bootstrap": "^0.32.4",
